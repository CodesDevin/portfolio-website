function isLoggedIn(){var userType=document.cookie.replace(/(?:(?:^|.*;\s*)UserType\s*\=\s*([^;]*).*$)|^.*$/,"$1");var vanityName=document.cookie.replace(/(?:(?:^|.*;\s*)vanityCookie\s*\=\s*([^;]*).*$)|^.*$/,"$1");if(userType==='Admin'||userType==='User'){var myA=document.querySelector('#loginIcon>a');let myNameLabel=document.getElementById('nameLabel');let myNameLabelMobile=document.getElementById('nameLabelMobile');myA.setAttribute('id','accountDropdown');myA.setAttribute('href','/account-dashboard/');function fadeOut(){myNameLabel.style.opacity=0;myNameLabelMobile.style.opacity=0;}
function fadeIn(){myNameLabel.style.opacity=1;myNameLabelMobile.style.opacity=1;}
function changeInnerHtml(){myNameLabel.innerHTML=vanityName;myNameLabelMobile.innerHTML=vanityName;console.log('fired');}
function changeText(){fadeOut();setTimeout(changeInnerHtml,500);setTimeout(fadeIn,500);}
changeText();var myImg=document.querySelector('#loginIcon>a>img');myImg.setAttribute('src','/wp-content/themes/FoundationPress/src/assets/images/icons/loggedIn.svg');myImg.setAttribute('alt','logged in icon');tippy('#accountDropdown',{theme:'light',placement:'bottom',arrow:true,animateFill:false,animation:'shift-away',interactive:true,content:'<ul><li><a href="/account-dashboard/">Dashboard</a></li><li><a href="/wp-login.php?action=logout">Log Out</a></li></ul>',});var myMobileA=document.querySelector('#loginIconMobile>a');myMobileA.setAttribute('href','/account-dashboard');var myMobileImg=document.querySelector('#loginIconMobile>a>img');myMobileImg.setAttribute('src','/wp-content/themes/FoundationPress/src/assets/images/icons/loggedIn.svg');myMobileImg.setAttribute('alt','logged in icon');myMobileImg.setAttribute('style','margin-top: -2px; width: 24px;');}}
document.addEventListener("DOMContentLoaded",function(){isLoggedIn();});