$(document).ready(function(){setTimeout(()=>{let myCurrentPage=document.getElementById('biltmore-body');let searchInstBox=$("#algolia-search-box .ais-search-box--input")[0];if(typeof searchInstBox!=="undefined"){if(!displayPaginationBar()){$("#algolia-pagination").hide();}else{$("#algolia-pagination").show();}
searchInstBox.classList.add('search-alg-biltbox');if(myCurrentPage.classList.contains('search-results')||myCurrentPage.classList.contains('search-no-results')){let position=searchInstBox.offsetTop+searchInstBox.offsetHeight;let scrollTo=position+200;window.scrollTo(0,scrollTo);}else{if(myCurrentPage.classList.contains('single-faq')){window.scrollTo(0,0);}}
searchInstBox.addEventListener('keyup',function(e){if(e.target.value===""){$('#algolia-stats').empty();$('#algolia-hits').empty();$('#algolia-pagination').empty();$('#typemenu').empty();}});decodeSearchResults(null);searchInstBox.addEventListener('keydown',function(e){if(e.keyCode==37){e.stopPropagation();}
if(!displayPaginationBar()){$("#algolia-pagination").hide();}else{$("#algolia-pagination").show();}});}
$(".clear-input-icon").click(function(e){searchInstBox.value="";searchInstBox.focus();});},500);});function decodeSearchResults(item){if(item!=null){let titleLength=item.length;let searchResult=item.indexOf(":");let emFirst=item.indexOf("&lt;em&gt;");let emSecond=item.indexOf("&lt;/em&gt;");let emTags={};if(emFirst>-1&&emSecond>-1){emTags={first:item.substring(0,emFirst),italicContent:item.substring(emFirst+10,emSecond),second:item.substring(emSecond+11,titleLength)};item="<span class='light-cont'>"+emTags.first+"</span>"+"<span class='italic-cont'>"+emTags.italicContent+"</span><span class='light-cont'>"+emTags.second+"</span>";emTags.first="";emTags.second="";emTags.italicContent="";emFirst=0;emSecond=0;}else{let searchTitle=item.substring(0,searchResult+1);let searchTitle2=item.substring(searchResult+1,titleLength);item="<span class='h2-cont'>"+searchTitle+"</span>"+"<span class='light-cont'>"+searchTitle2+"</span>";searchTitle="";searchTitle2="";}
return item;}}
function displayPaginationBar(){let myPaginationList=$("#algolia-pagination ul.ais-pagination .ais-pagination--item__page");if(myPaginationList.length<=1){return false;}else{return true;}}